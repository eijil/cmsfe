{
  "compilerOptions": {
    "allowJs": true,
    "declaration": false,
    "declarationMap": false,
    "incremental": true,
    "jsx": "preserve",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "noEmit": true,
    "target": "ESNext",
    "baseUrl": "./",
    "moduleResolution": "NodeNext",
    "resolveJsonModule": true,
    "paths": {
      "@@/*": [
        ".dumi/tmp/*"
      ]
    }
  },
  "include": [
    ".dumirc.ts"
  ]
}
